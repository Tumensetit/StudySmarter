{
  "name": "StudySmarter",
  "description": "Extension for study improvement",
  "version": "0.1",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_icon": "images.png",
    "default_title": "StudySmarter"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["tabs", "activeTab", "webNavigation", "notifications"]
}
